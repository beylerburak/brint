{
    "name": "@brint/shared-config",
    "version": "0.1.0",
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./upload": {
            "types": "./dist/upload.d.ts",
            "import": "./dist/upload.mjs",
            "require": "./dist/upload.js"
        },
        "./plans": {
            "types": "./dist/plans.d.ts",
            "import": "./dist/plans.mjs",
            "require": "./dist/plans.js"
        },
        "./platform-rules": {
            "types": "./dist/platform-rules.d.ts",
            "import": "./dist/platform-rules.mjs",
            "require": "./dist/platform-rules.js"
        },
        "./queue-rules": {
            "types": "./dist/queue-rules.d.ts",
            "import": "./dist/queue-rules.mjs",
            "require": "./dist/queue-rules.js"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsup src/index.ts src/upload.ts src/plans.ts src/platform-rules.ts src/queue-rules.ts src/user-settings.ts --format cjs,esm --dts --external zod",
        "dev": "tsup src/index.ts src/upload.ts src/plans.ts src/platform-rules.ts src/queue-rules.ts src/user-settings.ts --format cjs,esm --dts --watch --external zod",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "zod": "^3.25.76"
    }
}